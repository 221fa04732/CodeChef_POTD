#include <bits/stdc++.h>
using namespace std;
int main() 
{
	int t;
	cin>>t;
	while(t--)
	{
	    int a,b;
	    cin>>a>>b;
	    int c=0,d=0,e=a,f=b;
	    while(e)
	    {
	        c=(c*10+e%10);
	        d=(d*10+f%10);
	        e/=10;
	        f/=10;
	    }
	    if(a>b || a>d || c>b || c>d)
	    {
	        cout<<"YES"<<"\n";
	    }
	    else 
	    {
	        cout<<"NO"<<"\n";
	    }
	}
}
